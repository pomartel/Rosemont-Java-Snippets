{
  "name": "rosemont-java-snippets",
  "displayName": "Rosemont Java Configuration",
  "description": "Extension pour les cours d'introduction à la programmation en Java au Cégep Rosemont.",
  "version": "1.1.3",
  "engines": {
    "vscode": "^1.80.0"
  },
  "categories": [
    "Snippets"
  ],
  "icon": "icons/logo-rosemont.png",
  "publisher": "CegepRosemont",
  "author": {
    "name": "Pierre Olivier Martel",
    "email": "pmartel@crosemont.qc.ca",
    "url": "https://github.com/pomartel"
  },
  "repository": {
    "url": "https://github.com/pomartel/rosemont-java-snippets",
    "type": "git"
  },
  "contributes": {
    "snippets": [
      {
        "language": "java",
        "path": "./snippets/snippets.code-snippets"
      }
    ],
    "keybindings": [
      {
        "command": "git.clone",
        "key": "ctrl+shift+c",
        "when": "!operationInProgress"
      }
    ],
    "configurationDefaults": {
      "editor.minimap.enabled": false,
      "files.autoSave": "onFocusChange",
      "files.trimTrailingWhitespace": true,
      "git.autofetch": true,
      "git.enableSmartCommit": true,
      "git.confirmEmptyCommits": false,
      "git.postCommitCommand": "push",
      "editor.formatOnSave": true,
      "editor.formatOnPaste": true,
      "[java]": {
        "java.inlayHints.parameterNames.enabled": "none",
        "redhat.telemetry.enabled": true,
        "errorLens.enabledDiagnosticLevels": [
          "error"
        ]
      }
    }
  },
  "extensionDependencies": [
    "ms-ceintl.vscode-language-pack-fr",
    "redhat.java",
    "vscjava.vscode-java-debug",
    "usernamehw.errorlens",
    "visualstudioexptteam.vscodeintellicode"
  ]
}